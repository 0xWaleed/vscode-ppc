{
    "name": "vcode-ppc",
    "displayName": "vscode-ppc",
    "description": "PowerPC Assembly Language Support",
    "version": "0.0.1",
    "publisher": "zachsez",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "powerpc",
            "aliases": ["PowerPC Assembly", "powerpc"],
            "extensions": [".S",".s",".asm",".ppc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "powerpc",
            "scopeName": "source.powerpc",
            "path": "./syntaxes/powerpc.tmLanguage.json"
        }]
    },
    "repository": {
        "url": "https://github.com/zachsez/vscode-ppc",
        "type": "git"
    }
}